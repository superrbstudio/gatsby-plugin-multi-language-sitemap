{
  "name": "gatsby-plugin-multi-language-sitemap",
  "description": "Gatsby plugin that automatically creates a sitemap supporting multi-language for your site",
  "version": "1.1.0",
  "homepage": "https://github.com/SteveYuOWO/gatsby-plugin-multi-language-sitemap",
  "repository": {
    "type": "git",
    "url": "https://github.com/SteveYuOWO/gatsby-plugin-multi-language-sitemap"
  },
  "bugs": {
    "url": "https://github.com/SteveYuOWO/gatsby-plugin-multi-language-sitemap/issues"
  },
  "dependencies": {
    "@babel/runtime": "^7.14.6",
    "common-tags": "^1.8.0",
    "minimatch": "^3.0.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@types/jest": "^26.0.24",
    "babel-preset-gatsby-package": "^1.10.0-next.0",
    "cross-env": "^7.0.3",
    "gatsby": "^3.0.0-next.0",
    "gatsby-plugin-utils": "^1.10.0-next.0",
    "git-cz": "^4.7.6",
    "jest": "^27.0.6",
    "react": "^16.9.0 || ^17.0.0",
    "react-dom": "^16.9.0 || ^17.0.0"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "multi-lang",
    "multilingual",
    "multi-language"
  ],
  "license": "MIT",
  "main": "index.js",
  "peerDependencies": {
    "gatsby": "^3.0.0-next.0 || ^4.0.0 || ^5.0.0",
    "react": "^16.9.0 || ^17.0.0",
    "react-dom": "^16.9.0 || ^17.0.0"
  },
  "scripts": {
    "start": "node index",
    "clean": "rm -rf public build",
    "build": "babel src --out-dir ./ --ignore \"**/__tests__\"",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore \"**/__tests__\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "cz": "git-cz"
  },
  "engines": {
    "node": ">=12.13.0"
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  }
}
